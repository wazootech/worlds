{
  "name": "@fartlabs/worlds",
  "version": "0.0.10",
  "exports": "./src/sdk/mod.ts",
  "imports": {
    "#/": "./src/",
    "@comunica/query-sparql-rdfjs-lite": "npm:@comunica/query-sparql-rdfjs-lite@^5.0.1",
    "@fartlabs/rt": "jsr:@fartlabs/rt@^0.0.11",
    "@kitsonk/kv-toolbox": "jsr:@kitsonk/kv-toolbox@^0.30.0",
    "@olli/kvdex": "jsr:@olli/kvdex@^3.4.2",
    "@std/assert": "jsr:@std/assert@^1.0.16",
    "@types/n3": "npm:@types/n3@^1.26.1",
    "n3": "npm:n3@^2.0.1",
    "zod": "npm:zod@^4.3.5"
  },
  "tasks": {
    "start": "deno serve -A --unstable-kv --env --unstable-raw-imports src/server/main.ts",
    "test": "deno test -A --unstable-kv --env-file=.env.example --unstable-raw-imports",
    "bench": "deno bench -A --unstable-raw-imports",
    "fmt": "deno fmt",
    "lint": "deno lint --unstable-kv --unstable-raw-imports",
    "precommit": {
      "description": "Runs fmt, lint, and test",
      "dependencies": [
        "test",
        "lint",
        "fmt"
      ]
    }
  }
}
