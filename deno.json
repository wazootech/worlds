{
  "name": "@fartlabs/worlds",
  "version": "0.0.1",
  "exports": "./src/v1/sdk/mod.ts",
  "imports": {
    "#/": "./src/",
    "@fartlabs/rt": "jsr:@fartlabs/rt@^0.0.11",
    "@std/fs": "jsr:@std/fs@^1.0.20",
    "@std/http": "jsr:@std/http@^1.0.22",
    "@std/path": "jsr:@std/path@^1.1.3",
    "@std/assert": "jsr:@std/assert@^1.0.16",
    "@std/streams": "jsr:@std/streams@^1.0.14",
    "@std/text": "jsr:@std/text@^1.0.16",
    "oxigraph": "npm:oxigraph@^0.5.2",
    "zod": "npm:zod@^4.1.13"
  },
  "tasks": {
    "start": "deno serve -A --env --unstable-kv src/main.ts",
    "test": "deno test -A --unstable-kv --env-file=.env.example",
    "bench": "deno bench -A",
    "fmt": "deno fmt",
    "lint": "deno lint --unstable-kv",
    "precommit": {
      "description": "Runs fmt, lint, and test",
      "dependencies": [
        "test",
        "lint",
        "fmt"
      ]
    }
  }
}
